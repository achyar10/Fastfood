	<queryString>
		<![CDATA[SELECT
     detil_transaksi.`detil_transaksi_id` AS detil_transaksi_id_detil_transaksi,
     detil_transaksi.`transaksi_id` AS detil_transaksi_transaksi_id,
     detil_transaksi.`paket_kode` AS detil_transaksi_paket_kode,
     detil_transaksi.`detil_transaksi_harga` AS detil_transaksi_harga,
     detil_transaksi.`detil_transaksi_jumlah` AS detil_transaksi_jumlah,
     detil_transaksi.`detil_transaksi_total` AS detil_transaksi_total_harga,
     paket.`paket_nama` AS paket_makanan_nama_paket,
     transaksi.`transaksi_id` AS transaksi_transaksi_id,
     date_format(transaksi.`transaksi_tanggal`,"%d %M %Y") AS transaksi_tanggal,
     pengguna.`pengguna_nama` AS pengguna_nama_pengguna
FROM
     `detil_transaksi` detil_transaksi INNER JOIN `transaksi` transaksi ON detil_transaksi.`transaksi_id` = transaksi.`transaksi_id`
     INNER JOIN `paket` paket ON detil_transaksi.`paket_kode` = paket.`paket_kode`
     INNER JOIN `pengguna` pengguna ON transaksi.`pengguna_id` = pengguna.`pengguna_id`
WHERE
detil_transaksi.`transaksi_id` = $P{idtransaksi}]]>
